<% form_for @contact, :url => { :action => "create" }, :html => { :method => :post } do |f| %>
  <p>
    <%= f.label "Name" %>
  </p>
  <p>
    <%= f.text_field :sender_name %>
  </p>
  <p>
    <%= f.label "Email" %>
  </p>
  <p>
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label "Event Date" %>
  </p>
  <p>
    <%= f.text_field :event_date, :id => "datepicker" %>
  </p>
  <p>
    <%= f.label "Details" %>
  </p>
  <p>
    <%= f.text_area( :content, :cols => 60, :rows => 3 ) %>
  </p>
  <p>
    <%= f.submit "Submit" %>
  </p>
<% end %>
